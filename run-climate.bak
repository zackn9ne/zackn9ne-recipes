apps/Slack.recipe \
apps/Dropbox.recipe \
apps/Firefox.recipe \
apps/Zoom.recipe \
apps/Filestream.recipe \
--prefs /Users/test/Desktop/Autopkgr-Prefs/$ENV.json \
--key STOP_IF_NO_JSS_UPLOAD=False

echo "finished run: $(date)"
