if [ -e "$LOGFILE" ]
      then
	echo "$LOGFILE Exists"
   else
	echo "Creating $LOGFILE"
	touch $LOGFILE
fi

exec 3>&1 4>&2
trap 'exec 2>&4 1>&3' 0 1 2 3
exec 1>$LOGFILE 2>&1

